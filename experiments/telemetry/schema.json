        }]
      },
      {
        "name": "recordEvent",
        "type": "function",
        "description": "Record a Telemetry event",
        "async": false,
        "parameters": [{
        	"name": "payload",
        	"type": "object",
        	"properties": {
        	  "iteration": { "type": "number" },
        	  "loss": { "type": "number" },
        	  "numCharsTyped": { "type": "number" },
        	  "numSuggestions": { "type": "number" },
        	  "rankSelected": { "type": "number" },
        	  "weights": { "type": "array", "items": { "type": "number" } },

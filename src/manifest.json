{
  "name": "Federated Learning Awesome Bar",
  "description": "Improve history and bookmark search",
  "version": "2.0.0",
  "manifest_version": 2,
  "hidden": true,
  "applications": {
    "gecko": {
      "id": "federated-learning-v2@shield.mozilla.org",
      "strict_min_version": "65.0a1"
    }
  },
  "default_locale": "en-US",
  "experiment_apis": {
    "awesomeBar": {
      "schema": "./privileged/awesomeBar/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/awesomeBar/api.js",
        "paths": [["experiments", "awesomeBar"]]
      }
    },
    "frecency": {
      "schema": "./privileged/frecency/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/frecency/api.js",
        "paths": [["experiments", "frecency"]]
      }
    },
    "prefs": {
      "schema": "./privileged/prefs/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/prefs/api.js",
        "paths": [["experiments", "prefs"]]
      }
    },
    "privacyContext": {
      "schema": "./privileged/privacyContext/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/privacyContext/api.js",
        "paths": [["privacyContext"]]
      }
    },
    "study": {
      "schema": "./privileged/study/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/study/api.js",
        "paths": [["study"]]
      }
    }
  },
  "permissions": [
    "webRequest", "<all_urls>", "management", "storage", "alarms"
  ],
  "background": {
    "scripts": ["lib/prefs.js", "lib/synchronization.js", "lib/optimization.js", "studySetup.js", "feature.js", "background.js"]
  },
  "icons": {
    "48": "icons/shield-icon.48.png",
    "96": "icons/shield-icon.98.png"
  }
}
